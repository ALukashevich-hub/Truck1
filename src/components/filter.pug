        input.input.input-width(type="text" placeholder="Min")
        input.input.input-width(type="text" placeholder="Max")
  div.card.card-margin
    header.card-header
      p.card-header-title Param #4
      a.card-header-icon.is-hidden-fullscreen(href="#collapsible-card" data-action="collapse" aria-label="more options")
