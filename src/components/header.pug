nav.navbar(role="navigation" aria-label="main navigation")
  div.navbar-brand
    a.navbar-item(href="#")
      img.logo(src=require("/src/assets/images/truck.jpg") alt="Лого Truck1")
div.navbar-menu
  div.navbar-start
    each nav in htmlWebpackPlugin.options.templateParameters.nav
      a.navbar-item(href=nav.href)= nav.text
nav.breadcrumb.has-succeeds-separator(aria-label="breadcrumbs")
  ul
    each breadcrumb, index in htmlWebpackPlugin.options.templateParameters.breadcrumbs
      if index != htmlWebpackPlugin.options.templateParameters.breadcrumbs.length-1
        li
          a(href=breadcrumb.href)= breadcrumb.text
      else
        li.is-active
          a(href=breadcrumb.href)= breadcrumb.text